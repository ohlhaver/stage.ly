<tr onclick="location.href='<%=event_path(very_short_event)%>';" style="cursor:pointer">
        <td>
<div class = "very_short_event_listing" >

	<div class="span8">
		<% if very_short_event.works.any? %>
			<%= render very_short_event.works %>
		<%else%>
			<%= render very_short_event.artists %>
		<%end%>
	</div>

	<div class="span4">
		<%= I18n.localize(very_short_event.time, :format => :up) %> <br>
		<%= I18n.localize(very_short_event.time, :format => :down) %>
	</div>


</div>
</td>
</tr>

